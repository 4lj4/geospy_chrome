{
    "manifest_version": 3,
    "name": "Locate with GeoSpy",
    "version": "1.0",
    "description": "Geolocate images using Geospy from graylark technologies LLC inc. Developed by LJ4",
    "permissions": [
        "contextMenus",
        "activeTab",
        "storage",
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "*://*/*" 
    ],
    "icons": {
        "48": "icon.png"
    },
    "background": {
        "service_worker": "service_worker.js",
        "type": "module"
    }
}
